{
    "name": "lwsoftbd/lw-settings",
    "description": "A reusable Laravel package for managing site settings",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mithun Sarker",
            "email": "dev@lwsoftbd.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "LWSoftBD\\LwSettings\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "LWSoftBD\\LwSettings\\LwSettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}